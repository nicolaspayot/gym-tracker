{
  "name": "gym-tracker-api",
  "version": "1.0.0",
  "description": "Simple CRUD API to handle gym workout routines",
  "main": "app.js",
  "scripts": {
    "eslint": "node node_modules/eslint/bin/eslint src/*.js; exit 0",
    "eslint:watch": "node node_modules/eslint-watch/bin/esw -w src/*.js; exit 0",
    "serve": "node node_modules/nodemon/bin/nodemon babel src/app.js",
    "start": "node node_modules/parallelshell 'npm run serve' 'npm run eslint:watch'",
    "test": "node node_modules/babel-tape-runner/bin/babel-tape-runner test/*.test.js | faucet",
    "pretest": "npm run eslint"
  },
  "author": "Nicolas Payot",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-tape-runner": "^1.2.0",
    "connect": "^3.4.0",
    "eslint": "^1.5.1",
    "eslint-watch": "^2.1.1",
    "faucet": "0.0.1",
    "lodash": "^3.10.1",
    "nodemon": "^1.7.0",
    "parallelshell": "^2.0.0",
    "tape": "^4.2.1"
  }
}
